{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Infinity Busines Profile",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "_id": {
        "type": "string",
        "format": "uuid",
        "coerce": true
      },
      "userwallet": {
        "type": "string"
      },
      "name": {
        "type": "object",
        "properties": {
          "$share": {
            "type": "string"
          }
        },
        "required": [
          "$share"
        ]
      },
      "industry": {
        "type": "object",
        "properties": {
          "$share": {
            "type": "string"
          }
        },
        "required": [
          "$share"
        ]
      },
      "description": {
        "type": "object",
        "properties": {
          "$share": {
            "type": "string"
          }
        },
        "required": [
          "$share"
        ]
      },
      "merchantwalletaddress": {
        "type": "object",
        "properties": {
          "$share": {
            "type": "string"
          }
        },
        "required": [
          "$share"
        ]
      },
      "tokenname": {
        "type": "object",
        "properties": {
          "$share": {
            "type": "string"
          }
        },
        "required": [
          "$share"
        ]
      },
      "logo": {
        "type": "string"
      },
      "banner": {
        "type": "string"
      }
    },
    "required": [
      "_id",
      "userwallet",
      "name",
      "industry",
      "description",
      "merchantwalletaddress",
      "tokenname",
      "logo",
      "banner"
    ]
  }
}